PFont font ;
PFont font2 ;


void setup() {
  size (600, 500) ;
  background (255) ;
  smooth() ;
  font = loadFont ("ChaparralPro-Bold-48.vlw") ;
  font2 = loadFont ("Narkisim-48.vlw") ;
}

void draw() {
  fill (190,0,0) ;
  textFont (font) ;
  text ("Blaine Williamson" , 40, 100) ;
  fill (0, 0, 190) ;
  textFont (font2) ;
  text ("Blaine Williamson" , 40, 300) ;
}
